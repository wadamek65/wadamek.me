---
import ProfilePictureImage from '../media/profile_picture.png';
import { Image } from 'astro:assets';

const { width, height } = Astro.props;
---

<Image
	width={width ?? 120}
	height={height ?? 120}
	class="rounded-full border-2 border-st-tropaz-700"
	src={ProfilePictureImage}
	loading="eager"
	alt=""
/>
